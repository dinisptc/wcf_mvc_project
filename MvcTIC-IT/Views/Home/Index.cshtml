<h3>@Html.ActionLink("España", "Index", "Espanha") : @es.Count() @ViewRes.layout.jobofeers</h3>
<br />
<h3>@Html.ActionLink("France", "Index", "France") : @fr.Count() @ViewRes.layout.jobofeers</h3>
<br />
<h3>@Html.ActionLink("UK", "Index", "UK") : @uk.Count() @ViewRes.layout.jobofeers</h3>
<br />
<h3>@Html.ActionLink("Deutschland", "Index", "Alemanha"): @al.Count() @ViewRes.layout.jobofeers</h3>
<br />
<h3>@Html.ActionLink("World", "Index", "World") : @ww.Count() @ViewRes.layout.jobofeers</h3>