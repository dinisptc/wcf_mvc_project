@{
    ViewBag.Title = "Index";
}

@{
    MvcTIC_IT.ServiceRefPortugal.ServicePortugalClient proxy = new MvcTIC_IT.ServiceRefPortugal.ServicePortugalClient();
    var pt = proxy.GetAllAnuncios();

    MvcTIC_IT.ServiceRefEspanha.ServiceEspanhaClient proxyes = new MvcTIC_IT.ServiceRefEspanha.ServiceEspanhaClient();
    var es = proxyes.GetAllAnuncios();

    MvcTIC_IT.ServiceRefFrance.ServiceFranceClient proxyfr = new MvcTIC_IT.ServiceRefFrance.ServiceFranceClient();
    var fr = proxyfr.GetAllAnuncios();

    MvcTIC_IT.ServiceRefUK.ServiceUKClient proxyuk = new MvcTIC_IT.ServiceRefUK.ServiceUKClient();
    var uk = proxyuk.GetAllAnuncios();


    MvcTIC_IT.ServiceRefAlemanha.ServiceAlemanhaClient proxyal = new MvcTIC_IT.ServiceRefAlemanha.ServiceAlemanhaClient();
    var al = proxyal.GetAllAnuncios();

    MvcTIC_IT.ServiceRefWorld.ServiceWorldClient proxyww = new MvcTIC_IT.ServiceRefWorld.ServiceWorldClient();
    var ww = proxyww.GetAllAnuncios();
    
}

<h3>@Html.ActionLink("Portugal", "Index", "Portugal") : @pt.Count() @ViewRes.layout.jobofeers</h3>
